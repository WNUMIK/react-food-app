IsBumping, setBtnIsBumping] = useState(false);\n    const cartCtx = useContext(CartContext);\n    const {items} = cartCtx;\n\n    const numberOfCartItems = items.reduce((currNumber, item) => {\n        return currNumber + item.amount;\n    }, 0)\n\n\n    const btnClasses = `${classes.button} ${btnIsBumping ? classes.bump : ''}`;\n\n    useEffect(() => {\n        if (cartCtx.items.length === 0) {\n            return;\n        }\n        setBtnIsBumping(true);\n        const timer = setTimeout(() => {\n            setBtnIsBumping(false);\n        }, 300);\n\n        return () => {\n            clearTimeout(timer);\n        };\n\n    }, [items]);\n    return (\n        <button className={btnClasses} onClick={props.onClick}>\n            <span className={classes.icon}>\n                <CartIcon/>\n            </span>\n            <span className={classes.span}>Your Cart</span>\n            <span className={classes.badge}>{numberOfCartItems}</span>\n        </button>\n    );\n};\n\nexport default HeaderCartButton;","/Users/k1wsatz/programowanie/react-udemy/project_5/src/components/Cart/CartIcon.js",[],"/Users/k1wsatz/programowanie/react-udemy/project_5/src/components/Meals/Meals.js",[],"/Users/k1wsatz/programowanie/react-udemy/project_5/src/components/Meals/AvailableMeals.js",[],"/Users/k1wsatz/programowanie/react-udemy/project_5/src/components/Meals/MealsSummary.js",[],"/Users/k1wsatz/programowanie/react-udemy/project_5/src/components/UI/Card.js",[],"/Users/k1wsatz/programowanie/react-udemy/project_5/src/components/Meals/MealItem/MealItem.js",[],"/Users/k1wsatz/programowanie/react-udemy/project_5/src/components/Meals/MealItem/MealItemForm.js",[],"/Users/k1wsatz/programowanie/react-udemy/project_5/src/components/UI/Input.js",[],"/Users/k1wsatz/programowanie/react-udemy/project_5/src/components/Cart/Cart.js",[],"/Users/k1wsatz/programowanie/react-udemy/project_5/src/components/UI/Modal.js",[],"/Users/k1wsatz/programowanie/react-udemy/project_5/src/store/CartProvider.js",